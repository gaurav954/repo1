<div id="myleave">
<table>
    <tr style="background: lightslategrey;">
        <th>Leave Type</th>
        <th>Accrued</th>
        <th>Availed</th>
        <th>Balance</th>
    </tr>
    
    <tr>
        <th>Earned Leave</th>
        <td>${EL.Acc}</td>
        <td>${EL.Ava}</td>
        <td>${EL.Bal}</td>
    </tr>

    <tr>
      <th>Sick Leave</th>
      <td>${SK.Acc}</td>
      <td>${SK.Ava}</td>
      <td>${SK.Bal}</td>
    </tr>

    <tr>
      <th>Compensatory Leave</th>
      <td>${CL.Acc}</td>
      <td>${CL.Ava}</td>
      <td>${CL.Bal}</td>
    </tr>

    <tr>
      <th>Leave Without Pay</th>
      <td>${LW_Pay.Acc}</td>
      <td>${LW_Pay.Ava}</td>
      <td>${LW_Pay.Bal}</td>
    </tr>

    <tr>
      <th>Holiday Worked</th>
      <td>${Holiday_Worked.Acc}</td>
      <td>${Holiday_Worked.Ava}</td>
      <td>${Holiday_Worked.Bal}</td>
    </tr>

    <tr>
      <th>Paid Time Off</th>
      <td>${Paid_Time_Off.Acc}</td>
      <td>${Paid_Time_Off.Ava}</td>
      <td>${Paid_Time_Off.Bal}</td>
    </tr>

    <tr>
      <th>Worked From Home</th>
      <td>${Work_From_Home.Acc}</td>
      <td>${Work_From_Home.Ava}</td>
      <td>${Work_From_Home.Bal}</td>
    </tr>

    <tr>
      <th>Paternity Leave</th>
      <td>${Paternity_Leave.Acc}</td>
      <td>${Paternity_Leave.Ava}</td>
      <td>${Paternity_Leave.Bal}</td>
    </tr>

    <tr>
      <th>Bereavement Leave</th>
      <td>${Bereavement_Leave.Acc}</td>
      <td>${Bereavement_Leave.Ava}</td>
      <td>${Bereavement_Leave.Bal}</td>
    </tr>

    <tr>
      <th>Wedding Day Leave</th>
      <td>${WeddingDay_Leave.Acc}</td>
      <td>${WeddingDay_Leave.Ava}</td>
      <td>${WeddingDay_Leave.Bal}</td>
    </tr>
</table>
</div>

